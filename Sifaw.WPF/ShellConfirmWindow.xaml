<Window x:Class="Sifaw.WPF.ShellConfirmWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ShellWindow" 
        Width="800" 
        Height="600"
        WindowStartupLocation="CenterScreen" 
        Loaded="Window_Loaded"
        Closing="Window_Closing"
        xmlns:my="clr-namespace:Sifaw.WPF">
    <my:ShellConfirmControl x:Name="shell" Confirm="shell_Confirm" Cancel="shell_Cancel" />
</Window>
